{{#each flashMessages.queue as |flash|}}
  {{flash-message flash=flash}}
{{/each}}

<div class="row margin-bottom-md">
  <div class="col-md-12">
    <h3>{{model.name}}</h3>
  </div>
</div>
<div class="row">
  <div class="col-md-2">
    {{ group-menu model=model }}
  </div>
  <div class="col-md-10">
    {{outlet}}
  </div>
</div>