<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">{{ group.name }}</h3>
  </div>
  <div class="panel-body">
    <table class="table table-bordered">
      <thead>
        <tr>
          <th>Base Name</th>
          <th>Template Name</th>
          <th>Metric Type</th>
          <th>Policy Type</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <td>{{group.baseName}}</td>
          <td>{{log group}}</td>
          <td>{{group.metricType}}</td>
          <td>{{group.policyType}}</td>
        </tr>
      </tbody>
    </table>
  </div>

</div>